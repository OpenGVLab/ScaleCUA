      {
        "type": "cmd",
        "parameters": {
          "command": [
            "touch ~/Documents/test1.txt ~/Documents/test2.txt ~/Documents/test3.txt"
          ]
        }
      }
    ],
    "related_apps": [
      "terminal",
      "notes"
    ],
    "evaluator": {
        "func": ["notes_list_attachment_names_by_note_name"],
        "conj": "and",
        "parameters": [
          {
            "note_name": "document"
          }
        ],
        "expected": [
          {
          "type": "common",
          "rules": {
            "func": "find_str_in_list",
            "parameters": [["test1.txt", "test2.txt", "test3.txt"]]
          }
          }
        ]
      }
}